    <attr name="wc_stroke_color" format="color"/>
    <attr name="wc_day_text_color" format="color"/>
    <attr name="wc_row_text_color" format="color"/>
    <attr name="wc_background_day_color" format="color"/>
    <attr name="wc_stroke_width" format="integer"/>
    <attr name="wc_day_height" format="dimension"/>
    <attr name="wc_corner_radii" format="dimension"/>
