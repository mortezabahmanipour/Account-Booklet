  <declare-styleable name="RatingReviews">
    <attr name="rr_label_text_size" format="dimension"/>
    <attr name="rr_label_text_color" format="color"/>
    <attr name="rr_label_width" format="dimension"/>
    <attr name="rr_background_color" format="color"/>
    <attr name="rr_height" format="dimension"/>
    <attr name="rr_radius" format="dimension"/>
    <attr name="rr_space" format="dimension"/>
    <attr name="rr_number_reviews" format="integer"/>
    <attr name="rr_duration" format="integer"/>
  </declare-styleable>

