<resources>
  <declare-styleable name="Theme">
    <attr name="t_selectable" format="boolean"/>
    <attr name="t_background_color" format="color"/>
    <attr name="t_stroke_width" format="dimension"/>
    <attr name="t_stroke_color" format="color"/>
    <attr name="t_radii" format="dimension"/>
    <attr name="t_style">
      <enum name="none" value="0"/>
      <enum name="rect" value="1"/>
      <enum name="oval" value="2"/>
    </attr>
    <attr name="t_selector_color" format="color"/>
    <attr name="t_select_stroke_width" format="dimension"/>
    <attr name="t_select_stroke_color" format="color"/>
    <attr name="t_select_radii" format="dimension"/>
    <attr name="t_select_style">
      <enum name="none" value="0"/>
      <enum name="rect" value="1"/>
      <enum name="oval" value="2"/>
    </attr>
    <attr name="t_ripple" format="boolean"/>
    <attr name="t_disable_shape">
      <flag name="top" value="0x00000001"/>
      <flag name="bottom" value="0x00000002"/>
      <flag name="left" value="0x00000004"/>
      <flag name="right" value="0x00000008"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="CircleImageView">
    <attr name="civ_border" format="boolean" />
    <attr name="civ_border_width" format="dimension" />
    <attr name="civ_border_color" format="color" />
    <attr name="civ_background_color" format="color" />
    <attr name="civ_shadow" format="boolean" />
    <attr name="civ_shadow_color" format="color" />
    <attr name="civ_shadow_radius" format="float" />
    <attr name="civ_shadow_gravity">
      <flag name="center" value="1" />
      <flag name="top" value="2" />
      <flag name="bottom" value="3" />
      <flag name="start" value="4" />
      <flag name="end" value="5" />
    </attr>
  </declare-styleable>

  <declare-styleable name="SelectorImageView">
    <attr name="siv_tint_color" format="color"/>
    <attr name="siv_square" format="boolean"/>
    <attr name="siv_alpha" format="float"/>
  </declare-styleable>

  <declare-styleable name="EditText">
    <attr name="et_text_many" format="boolean" />
    <attr name="et_background_color" format="color" />
    <attr name="et_radius" format="dimension" />
    <attr name="et_stroke_width" format="dimension" />
    <attr name="et_stroke_select_width" format="dimension" />
    <attr name="et_stroke_color" format="color" />
    <attr name="et_stroke_selected_color" format="color" />
  </declare-styleable>

  <declare-styleable name="TextView">
    <attr name="tv_moving" format="boolean"/>
    <attr name="tv_many" format="boolean"/>
    <attr name="tv_drawable_tint_color" format="color"/>
    <attr name="tv_drawable_size" format="dimension"/>
    <attr name="tv_drawable_alpha" format="float"/>
    <attr name="tv_square" format="boolean"/>
    <attr name="tv_face_style" format="enum">
      <enum name="normal" value="0"/>
      <enum name="bold" value="1"/>
      <enum name="italic" value="2"/>
      <enum name="bold_italic" value="3"/>
    </attr>
  </declare-styleable>

  <declare-styleable name="ResizeableTextView">
    <attr name="rtv_close_color" format="color"/>
    <attr name="rtv_open_color" format="color"/>
    <attr name="rtv_duration" format="integer"/>
  </declare-styleable>

  <declare-styleable name="ImageView">
    <attr name="shiv_border_color" format="color"/>
    <attr name="shiv_border_width" format="dimension"/>
    <attr name="shiv_border_alpha" format="float"/>
    <attr name="shiv_square" format="boolean"/>
    <attr name="shiv_circle" format="boolean"/>
    <attr name="shiv_radius" format="dimension"/>
  </declare-styleable>

  <declare-styleable name="SelectorLayout">
    <attr name="s_square" format="boolean"/>
    <attr name="s_foreground" format="boolean"/>
  </declare-styleable>

  <declare-styleable name="CropImageView">
    <attr name="cropGuidelines">
      <enum name="off" value="0"/>
      <enum name="onTouch" value="1"/>
      <enum name="on" value="2"/>
    </attr>
    <attr name="cropScaleType">
      <enum name="fitCenter" value="0"/>
      <enum name="center" value="1"/>
      <enum name="centerCrop" value="2"/>
      <enum name="centerInside" value="3"/>
    </attr>
    <attr name="cropShape">
      <enum name="rectangle" value="0"/>
      <enum name="oval" value="1"/>
    </attr>
    <attr name="cropAutoZoomEnabled" format="boolean"/>
    <attr name="cropMaxZoom" format="integer"/>
    <attr name="cropMultiTouchEnabled" format="boolean"/>
    <attr name="cropFixAspectRatio" format="boolean"/>
    <attr name="cropAspectRatioX" format="integer"/>
    <attr name="cropAspectRatioY" format="integer"/>
    <attr name="cropInitialCropWindowPaddingRatio" format="float"/>
    <attr name="cropBorderLineThickness" format="dimension"/>
    <attr name="cropBorderLineColor" format="color"/>
    <attr name="cropBorderCornerThickness" format="dimension"/>
    <attr name="cropBorderCornerOffset" format="dimension"/>
    <attr name="cropBorderCornerLength" format="dimension"/>
    <attr name="cropBorderCornerColor" format="color"/>
    <attr name="cropGuidelinesThickness" format="dimension"/>
    <attr name="cropGuidelinesColor" format="color"/>
    <attr name="cropBackgroundColor" format="color"/>
