    <attr name="cpb_background_progress_color" format="color"/>
    <attr name="cpb_foreground_progress_color" format="color"/>
    <attr name="cpb_rounded_corner" format="boolean"/>
    <attr name="cpb_touch_enabled" format="boolean"/>
