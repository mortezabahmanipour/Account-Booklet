    <attr name="pv_text_message_color" format="color"/>
    <attr name="pv_text_btn_color" format="color"/>
    <attr name="pv_btn_background_color" format="color"/>
    <attr name="pv_btn_height" format="dimension"/>
    <attr name="pv_progress_size" format="dimension"/>
    <attr name="pv_icon_size" format="dimension"/>
    <attr name="pv_text_title_size" format="float"/>
    <attr name="pv_text_message_size" format="float"/>
    <attr name="pv_text_btn_size" format="float"/>
    <attr name="pv_icon_alpha" format="float"/>
  </declare-styleable>

  <declare-styleable name="DLStoreRow">
    <attr name="dl_sv_icon" format="reference"/>
    <attr name="dl_sv_name" format="string"/>
  </declare-styleable>

  <declare-styleable name="ShadowView">
    <attr name="shv_start_color" format="color"/>
    <attr name="shv_center_color" format="color"/>
    <attr name="shv_end_color" format="color"/>
    <attr name="shv_orientation" format="enum">
